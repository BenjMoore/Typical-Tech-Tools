<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <compilation targetFramework="4.8" />
    <authentication mode="Forms">
      <forms loginUrl="~/Admin/AdminLogin" timeout="30" />
    </authentication>
    <authorization>
      <allow users="*" />
      <deny users="?" />
    </authorization>
  </system.web>
  <system.webServer>
    <modules>
      <remove name="WebDAVModule" />
    </modules>
    <defaultDocument>
      <files>
        <add value="index.html" />
